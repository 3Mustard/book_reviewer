form for new review 

<%= form_for @review do |f| %> 
    <%= f.fields_for :book do |b| %>
        <%= b.label :title %>
        <%= b.text_field :title 
    <%end%> 

    <%= f.label :rating, "rate 1-10" %>  
    <%= f.text_field :rating %> 
    
    <%= f.label :content %>
    <%= f.text_field :content %> 

    <%= f.submit %> 
<%end%>